{
    "name": "reflar/cookie-consent",
    "description": "Customizable cookie consent notice for your Flarum installation",
    "keywords": [
        "extension",
        "flarum",
        "partialdev",
        "customizable",
        "cookie",
        "consent"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Bond",
            "email": "joel.bond@com",
            "homepage": "https://com/"
        }
    ],
    "support": {
        "issues": "https://github.com/reflar/cookie-consent/issues",
        "source": "https://github.com/reflar/cookie-consent"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "extra": {
        "flarum-extension": {
            "title": "ReFlar Cookie Consent",
            "icon": {
                "name": "fas fa-cookie",
                "backgroundColor": "#263238",
                "color": "#FFF"
            }
        }
    },
    "flagrow": {
        "discuss": "https://discuss.flarum.org/d/10395-cookie-consent"
    },
    "autoload": {
        "psr-4": {
            "ReFlar\\CookieConsent\\": "src/"
        }
    }
}
